{
    "name": "cullymason/wordle-cli",
    "description": "A Prototype of Wordle",
    "keywords": ["termwind", "laravel", "php", "console", "cli"],
    "homepage": "https://cully.dev",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/cullymason/wordle-cli/issues",
        "source": "https://github.com/cullymason/wordle-cli"
    },
    "authors": [
        {
            "name": "Cully Mason",
            "email": "cully@hey.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-pspell": "*",

        "nunomaduro/termwind": "^1.4"
    },
    "require-dev": {
        "laravel-zero/framework": "^8.8",
        "mockery/mockery": "^1.4.3",
        "pestphp/pest": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/wordle-cli"]
}
